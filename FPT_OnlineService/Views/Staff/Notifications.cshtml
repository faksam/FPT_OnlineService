@model IEnumerable<FPT_OnlineService.Models.Notification>

@{
    ViewBag.Title = "Notifications";
    Layout = "~/Views/Shared/_StaffLayout.cshtml";
}

<h2>Notification</h2>

<div>
    <h4>Notification</h4>
    <hr />

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Staff.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Student.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Message)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Staff.UserId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Student.UserId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Message)
                </td>
                <td></td>
            </tr>
        }

    </table>
</div>
